.section {
  font-size: 32rpx;

}
.section__title {
  width: 100%;
  text-align: center;
}
.choose-button {
  width: 50%;
  background-color: #fff;
  border: none;
  float: left;
}
.date-list {
  width: 100%;
  height: 800rpx;
  background-color: #EDEDED;
  display: flex;
  display: -webkit-flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  padding-left: 10rpx;
}
.date-item {
  float: left;
  width: 105rpx;
  height: 105rpx;
  background-color: #fff;
  line-height: 105rpx;
  text-align: center;
  border-radius: 40rpx;
  margin-bottom: 6rpx;
}
.date-punch {
  background-color: #FFE4B5;
}
.clear-float {
  clear: both;
}
.week-list {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 32rpx;
  display: flex;
  display: -webkit-flex;
  flex-direction: row;
  flex-wrap: unset;
  border-top: 1px solid rgb(211, 211, 211);
  background-color: #EDEDED;
}
.week-item {
  flex: 1;
  text-align: center;
  width: 12rpx;
}