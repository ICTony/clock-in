<!--home:index.wxml-->
<view wx:if="{{false}}" class="container">
  <text>今天，你运动了吗？</text>
  <image src="{{logoUrl}}"></image>
  <text>
    <block wx:if="{{isPunch}}">你已经打卡了！</block>
    <block wx:else>还没有打卡哦！</block>
  </text>
  <view class="punch-view">
    <button bindtap="dayPunch" class="punch-button" wx:if="{{!isPunch}}">运动打卡</button>
    <button bindtap="dayPunch" class="punch-button" wx:else>取消打卡</button>
  </view>
</view>
<view>
  <view class='header'>
    <view class='circle'></view>
    <view class="wx-name">你的名字</view>
    <view class='notes'>
      <view>一年有三百六十五个日子</view>
      <view>五十二万五千分钟。</view>
    </view>
  </view>
  <view class='body'>
      买没有内容哦
  </view>
  <view class='footer'></view>
</view>